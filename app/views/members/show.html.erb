<div class="page-header">
  <h3>Member #<%= @member.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Email</dt>
      <dd><%= @member.email %></dd>

      <dt>Password</dt>
      <dd><%= @member.password %></dd>

      <dt>Name</dt>
      <dd><%= @member.name %></dd>

      <dt>Family</dt>
      <dd><%= @member.family_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/members" class="btn btn-primary">
        Back
      </a>
    </div>
  </div>
</div>
